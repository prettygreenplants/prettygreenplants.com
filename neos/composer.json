{
    "name": "prettygreenplants/prettygreenplants.com",
    "description": "Neos Base Distribution for Pretty Green Plants website.",
    "version": "2.0.0",
    "license": "GPL-3.0+",
    "support": {
        "email": "info@prettygreenplants.com"
    },
    "config": {
        "vendor-dir": "Packages/Libraries",
        "bin-dir": "bin",
        "allow-plugins": {
            "neos/composer-plugin": true,
            "composer/package-versions-deprecated": true
        }
    },
    "require": {
        "neos/neos": "~7.2.0",
        "neos/neos-ui": "~7.2.0",
        "neos/nodetypes": "~7.2.0",
        "neos/fusion-afx": "~7.2.0",
        "typo3fluid/fluid": "~2.7.0",

        "neos/seo": "~3.2",
        "flowpack/neos-frontendlogin": "~4.0",
        "neos/redirecthandler-neosadapter": "~4.3",
        "neos/redirecthandler-databasestorage": "~5.0",
        "neos/redirecthandler-ui": "~2.4",

        "networkteam/neos-mailobfuscator": "~3.3.0",
        "neos/googleanalytics": "~3.4.0",
        "flownative/neos-compressor": "~2.2"
    },
    "require-dev": {
        "neos/site-kickstarter": "~7.2.0",
        "neos/setup": "^5.0",
        "neos/neos-setup": "^1.1",

        "neos/buildessentials": "~7.2.0",
        "mikey179/vfsstream": "^1.6",
        "phpunit/phpunit": "^9.0",
        "symfony/css-selector": "~2.0",
        "neos/behat": "~7.2.0"
    },
    "suggest": {
        "ext-pdo_sqlite": "For running functional tests out-of-the-box this is required"
    },
    "scripts": {
        "post-update-cmd": "Neos\\Flow\\Composer\\InstallerScripts::postUpdateAndInstall",
        "post-install-cmd": "Neos\\Flow\\Composer\\InstallerScripts::postUpdateAndInstall",
        "post-package-update": "Neos\\Flow\\Composer\\InstallerScripts::postPackageUpdateAndInstall",
        "post-package-install": "Neos\\Flow\\Composer\\InstallerScripts::postPackageUpdateAndInstall"
    }
}
